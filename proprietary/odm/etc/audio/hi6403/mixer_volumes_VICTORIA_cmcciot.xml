<mixer>
	<!-- default digital pga values -->
	<ctl name="S1 IL PGA GAIN" value="0" />
	<ctl name="S1 IR PGA GAIN" value="0" />
	<ctl name="S2 IL PGA GAIN" value="0" />
	<ctl name="S2 IR PGA GAIN" value="0" />
	<ctl name="S3 IL PGA GAIN" value="0" />
	<ctl name="S3 IR PGA GAIN" value="0" />
	<ctl name="DACL PGA GAIN" value="110" />
	<ctl name="DACR PGA GAIN" value="110" />
	<ctl name="SIDE PGA GAIN" value="0" />
	<!-- default mixer volume values -->
	<ctl name="DACL MIXER S1L GAIN" value="3" />
	<ctl name="DACL MIXER S1R GAIN" value="3" />
	<ctl name="DACR MIXER S1L GAIN" value="3" />
	<ctl name="DACR MIXER S1R GAIN" value="3" />
	<!-- default analog pga values -->
	<ctl name="LINEL PGA GAIN" value="0" />
	<ctl name="LINER PGA GAIN" value="0" />
	<ctl name="AUX PGA GAIN" value="0" />
	<ctl name="MAIN PGA GAIN" value="0" />
	<ctl name="MAD PGA GAIN" value="13" />
	<ctl name="LOL PGA GAIN" value="14" />
	<ctl name="LOR PGA GAIN" value="14" />
	<ctl name="EP PGA GAIN" value="22" />
	<!-- default boost disable -->
	<ctl name="MAIN MIC BOOST0" value="0" />
	<ctl name="AUX MIC BOOST0" value="0" />
	<ctl name="LINEINL BOOST0" value="0" />
	<ctl name="LINEINR BOOST0" value="0" />
	<ctl name="ADC MAD BOOST0" value="0" />
	<ctl name="MAIN MIC BOOST1" value="0" />
	<ctl name="AUX MIC BOOST1" value="0" />
	<ctl name="LINEINL BOOST1" value="0" />
	<ctl name="LINEINR BOOST1" value="0" />
	<ctl name="ADC MAD BOOST1" value="0" />

	<!-- normal paths -->
	<path name="media-earpiece" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
		<ctl name="DACL MIXER S1L GAIN" value="1" />
		<ctl name="DACL MIXER S1R GAIN" value="1" />
		<ctl name="EP PGA GAIN" value="22" />
	</path>

	<path name="media-speaker" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
	</path>

	<path name="media-headphone" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
	</path>

	<path name="media-headset" >
		<path name="media-headphone" />
	</path>

	<path name="media-bt-sco" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
		<ctl name="DACL MIXER S1L GAIN" value="1" />
		<ctl name="DACL MIXER S1R GAIN" value="1" />
	</path>

	<path name="media-speaker-earpiece" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
		<ctl name="EP PGA GAIN" value="22" />
	</path>

	<path name="media-speaker-headset" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
		<ctl name="DACL PGA GAIN" value="64" />
		<ctl name="DACR PGA GAIN" value="64" />
	</path>

	<path name="media-speaker-headphone" >
		<path name="media-speaker-headset" />
	</path>

	<path name="communication-earpiece" >
		<path name="media-earpiece" />
		<ctl name="LINEL PGA GAIN" value="23" />
		<ctl name="LINER PGA GAIN" value="23" />
		<ctl name="AUX PGA GAIN" value="13" />
		<ctl name="MAIN PGA GAIN" value="13" />
	</path>

	<path name="communication-speaker" >
		<path name="media-speaker" />
		<ctl name="LINEL PGA GAIN" value="23" />
		<ctl name="LINER PGA GAIN" value="23" />
		<ctl name="AUX PGA GAIN" value="13" />
		<ctl name="MAIN PGA GAIN" value="13" />
	</path>

	<path name="communication-headset" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
		<ctl name="LINEL PGA GAIN" value="23" />
		<ctl name="LINER PGA GAIN" value="23" />
		<ctl name="AUX PGA GAIN" value="13" />
		<ctl name="MAIN PGA GAIN" value="13" />
	</path>

	<path name="communication-headphone" >
		<ctl name="S1 IL PGA GAIN" value="0" />
		<ctl name="S1 IR PGA GAIN" value="0" />
		<ctl name="LINEL PGA GAIN" value="23" />
		<ctl name="LINER PGA GAIN" value="23" />
		<ctl name="AUX PGA GAIN" value="13" />
		<ctl name="MAIN PGA GAIN" value="13" />
	</path>

	<path name="communication-speaker-earpiece" >
		<path name="media-speaker-earpiece" />
		<ctl name="LINEL PGA GAIN" value="23" />

<?xml version="1.0" encoding="iso-8859-1"?>
<Config>

	<Capture>
		<FrameStrategy>  <!-- 二进制序列，从左到右，分别表示副camera有无遮挡<1有，0无>，低亮度或者高亮度<1低照度，0高亮度>，是否小于右左sensor分辨率倍率<1小于等于，0大于> -->
			<Scene SceneIndex="0" ColorFrames="4" MonoFrames="4"/>
			<Scene SceneIndex="1" ColorFrames="4" MonoFrames="4"/>
			<Scene SceneIndex="2" ColorFrames="4" MonoFrames="4"/>
			<Scene SceneIndex="3" ColorFrames="4" MonoFrames="4"/>
			<Scene SceneIndex="4" ColorFrames="6" MonoFrames="0"/>
			<Scene SceneIndex="5" ColorFrames="6" MonoFrames="0"/>
			<Scene SceneIndex="6" ColorFrames="6" MonoFrames="0"/>
			<Scene SceneIndex="7" ColorFrames="6" MonoFrames="0"/>
			<Scene SceneIndex="8" ColorFrames="0" MonoFrames="6"/>
			<Scene SceneIndex="9" ColorFrames="0" MonoFrames="6"/>
			<Scene SceneIndex="10" ColorFrames="0" MonoFrames="6"/>
			<Scene SceneIndex="11" ColorFrames="0" MonoFrames="6"/>
		</FrameStrategy>
		<ISPControl>
			<ISO1>
				<isovalue>258</isovalue>
				<isSharpnessOn>0</isSharpnessOn>
				<isRawDenoiseOn>0</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>0</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO1>
			<ISO2>
				<isovalue>516</isovalue>
				<isSharpnessOn>0</isSharpnessOn>
				<isRawDenoiseOn>0</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>0</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO2>
			<ISO3>
				<isovalue>1200</isovalue>
				<isSharpnessOn>0</isSharpnessOn>
				<isRawDenoiseOn>0</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>0</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO3>
			<ISO4>
				<isovalue>1500</isovalue>
				<isSharpnessOn>0</isSharpnessOn>
				<isRawDenoiseOn>0</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>0</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO4>
			<ISO5>
				<isovalue>2500</isovalue>
				<isSharpnessOn>0</isSharpnessOn>
				<isRawDenoiseOn>0</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>0</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO5>
			<ISO6>
				<isovalue>3200</isovalue>
				<isSharpnessOn>1</isSharpnessOn>
				<isRawDenoiseOn>1</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>1</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO6>
			<ISO7>
				<isovalue>4800</isovalue>
				<isSharpnessOn>1</isSharpnessOn>
				<isRawDenoiseOn>1</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>1</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO7>
			<ISO8>
				<isovalue>20000</isovalue>
				<isSharpnessOn>1</isSharpnessOn>
				<isRawDenoiseOn>1</isRawDenoiseOn>
				<isYUVLumaDenoiseOn>1</isYUVLumaDenoiseOn>
				<isYUVChromaDenoiseOn>1</isYUVChromaDenoiseOn>
			</ISO8>
		</ISPControl>
		<OpticalZoom>	
			<MFSR>
				<CThr1>30</CThr1>
				<CThr2>50</CThr2>
				<MThr1>30</MThr1>
				<MThr2>50</MThr2>
			</MFSR>
		</OpticalZoom>
	</Capture>
	
	<RouteSwitchOZ>
		<CropRatio>5</CropRatio>
		
        <MonoOnlyChroma>5</MonoOnlyChroma>
        <MonoOnlyPixelPercent>99</MonoOnlyPixelPercent>
        <MonoOnlyTHVar1>1.5</MonoOnlyTHVar1>
        <MonoOnlyTHVar2>0.99</MonoOnlyTHVar2>
        <MonoOnlyDiffUMax>2</MonoOnlyDiffUMax>
        <MonoOnlyDiffVMax>2</MonoOnlyDiffVMax>
		
		<HybridEnlargeRatio>6</HybridEnlargeRatio>
		<HybridTH>4000</HybridTH>
		<DaylightISO>300</DaylightISO>	<!--  gain  300 -->
		<ROUTER0>				<!--  MONO_BASED   -->		
            <ColorFrames>2</ColorFrames>		
            <MonoFrames>4</MonoFrames>			
		</ROUTER0>
		<ROUTER1>			<!--  COLOR_BASED   -->		
            <ColorFrames>4</ColorFrames>		
            <MonoFrames>2</MonoFrames>	
		</ROUTER1>
		<ROUTER2>				<!--  COLOR_ONLY   -->		
            <ColorFrames>6</ColorFrames>		
            <MonoFrames>0</MonoFrames>	
		</ROUTER2>
		<ROUTER3>				<!--  HYBRID   -->		
            <ColorFrames>2</ColorFrames>		
            <MonoFrames>4</MonoFrames>	
		</ROUTER3>
		<ROUTER4>				<!--  MONO_ONLY   -->		
     
